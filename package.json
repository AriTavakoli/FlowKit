  "scripts": {
    "build": "node utils/build.js",
    "dev": "nodemon --exec ts-node server/index.ts --noEmit",
    "start": "node config/utils/webserver.js",
    "build:worker": "npm run build:typescript && npm run build:transferWorker",
    "build:typescript": "esbuild src/pages/Content/Service/worker.ts --bundle --outdir=src/internalDist --platform=node --target=es6 --loader:.js=jsx --loader:.tsx=tsx --sourcemap --define:process.env.NODE_ENV=\"production\"",
    "build:transferWorker": "esbuild src/internalDist/worker.js --bundle --outfile=dist/workerBundle.js --minify --platform=node --target=node12 --watch",
    "production": "webpack  --config config/prod/webpack.prod.js",
    "build:prod": "webpack --config config/prod/webpack.prod.js --mode production",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test": "jest",
    "replay": "replay recording.json",
    "test:WebflowExporter": "jest ./tests/Export/exporter.test.ts",
    "test:CssParser": "ts-node ./tests/Parser/cssParser.test.ts",
    "test:Blob:BlobGenerator": "jest ./tests/Blob/blobGenerator.test.ts",
    "test:Puppeteer": "jest ./tests/Puppeteer/pupp.test.ts",
    "test:Recordings": "npx @puppeteer/replay tests/all-recordings --headless=false"
  },
